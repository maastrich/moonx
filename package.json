{
  "name": "@maastrich/moonx",
  "version": "0.1.2",
  "description": "A CLI tool to help you with moon syntax",
  "keywords": [
    "moon",
    "moonrepo",
    "cli",
    "proto"
  ],
  "license": "MIT",
  "author": "Maastrich @maastrich",
  "type": "module",
  "main": "src/index.ts",
  "bin": {
    "moonx": "bin/moonx",
    "mx": "bin/moonx"
  },
  "files": [
    "src",
    "bin"
  ],
  "scripts": {
    "build": "bun build --compile . --outfile=\"bin/moonx\"",
    "postinstall": "bun run build",
    "prettier": "prettier --write .",
    "semver-check": "bun run scripts/semver-check.ts",
    "sort": "bun x sort-package-json"
  },
  "dependencies": {
    "@moonrepo/types": "^1.21.3",
    "cac": "^6.7.14",
    "chalk": "^5.3.0",
    "glob": "^10.3.10",
    "node-emoji": "^2.1.0",
    "nunjucks": "^3.2.4",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.2.1",
    "@types/nunjucks": "^3.2.6",
    "@types/semver": "^7.5.5",
    "@withfig/autocomplete-types": "^1.29.0",
    "bun-types": "^1.0.11",
    "prettier": "^3.0.3",
    "semver": "^7.6.3",
    "sort-package-json": "^2.6.0"
  }
}
